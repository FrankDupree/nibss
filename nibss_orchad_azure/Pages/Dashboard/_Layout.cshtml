

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <meta http-equiv="Content-Security-Policy"
          content="base-uri 'self';
               block-all-mixed-content;
               default-src 'self' https://drive.google.com;
               img-src 'self' data: https://*;
               object-src 'none';
               frame-src https://docs.google.com/
                         https://onedrive.live.com/;
               font-src https://cdnjs.cloudflare.com/ https://fonts.gstatic.com 'self';
               script-src https://cdnjs.cloudflare.com/
                          https://code.highcharts.com/
                           https://ajax.googleapis.com/
                          'self' 'unsafe-inline';
               style-src https://cdnjs.cloudflare.com/
                          https://fonts.googleapis.com/
                          https://fonts.gstatic.com/
                         'self'
                         'unsafe-inline';
               upgrade-insecure-requests;" />
    <link rel="shortcut icon" href="/img/favicon.png" type="image/x-icon" />
    <title>@ViewData["Title"] - NIBSS</title>
    <link href="~/lib/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/lib/css/materialize-icons.min.css" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="/lib/css/dash/vendors.min.css">
    <link rel="stylesheet" type="text/css" href="/lib/css/dash/all.min.css">
    <link rel="stylesheet" type="text/css" href="/lib/css/dash/materialize.min.css">
    <link rel="stylesheet" type="text/css" href="/lib/css/dash/style.min.css">
    <link rel="stylesheet" type="text/css" href="/css/dash/custom.min.css">
    <link rel="stylesheet" type="text/css" href="/lib/css/dash/datatables.min.css">
    @await RenderSectionAsync("Styles", false)
    <!-- Global site tag (gtag.js) - Google Analytics -->
<script async src="https://www.googletagmanager.com/gtag/js?id=G-SXR7X64YE3"></script>
<script>
  window.dataLayer = window.dataLayer || [];
  function gtag(){dataLayer.push(arguments);}
  gtag('js', new Date());

  gtag('config', 'G-SXR7X64YE3');
</script>
</head>
<body class="vertical-layout vertical-menu-collapsible page-header-dark vertical-modern-menu 2-columns  "
      data-open="click" data-menu="vertical-modern-menu" data-col="2-columns">

    <!-- BEGIN: SideNav-->
    @await Component.InvokeAsync("DashboardNav")
    <!-- END: SideNav-->
    <!-- BEGIN: Page Main-->
    <div id="main">
        <div class="row">

            <div class="breadcrumbs-dark pb-0 pt-4" id="breadcrumbs-wrapper" style="background: #2c5706;">
                <!-- Search for small screen-->
                <div class="container" style="padding-bottom:2.5%">
                    <div class="row">
                        <div class="col s10 m6 l6">
                            <h5 class="breadcrumbs-title mt-0 mb-0" id="page-title">Dashboard</h5>
                            <p id="page-sub-title" style="color: white">Data Loading...</p>
                        </div>
                        <div class="col s2 m6 l6">

                            <form action="account/logout" method="post">
                                @Html.AntiForgeryToken()
                                <button style="background: #9d7c13;" type="submit" class="btn waves-effect waves-light  breadcrumbs-btn right">LOGOUT</button>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col s12">
                <div class="container">
                    <div id="loader-div" class="row m-10 valign-wrapper">
                        <div class="col s12">
                            <h3 class="center-align header">Please Wait...</h3>
                        </div>
                    </div>
                    <div style="display:none" id="failed-div" class="row m-10 valign-wrapper dashboard-div">
                        <div class="col s12">
                            <h3 class="center-align header" style="color:darkred">Could not load data from source</h3><div class="center-align">
                                <form>
                                    <input class="btn" style="background:#9d7c13" type="button" onClick="history.go(0)" value="Refresh">
                                </form>
                            </div>

                        </div>
                    </div>

                    @RenderBody()

                </div>
            </div>
        </div>
        <!-- END: Page Main-->
        <!-- BEGIN: Footer-->

        <footer style="background: #2c5706;"
                class="page-footer  footer-dark">
            <div class="footer-copyright">
                <div class="container">
                    <span>
                        &copy; 2020 <a rel="noopener noreferrer" href="https://nibss-plc.com.ng/"
                                       target="_blank">NIBSS PLC</a> All rights reserved.
                    </span>
                </div>
            </div>
        </footer>

        <!-- END: Footer-->
    </div>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="/lib/js/dash/vendors.min.js" type="text/javascript"></script>
    <script src="/lib/js/dash/chart.min.js" type="text/javascript"></script>
    <script src="/lib/js/dash/all.min.js" type="text/javascript"></script>
    <script src="/lib/js/dash/plugins.min.js" type="text/javascript"></script>
    <script src="/lib/js/dash/cards-extended.min.js" type="text/javascript"></script>


    <script src="https://code.highcharts.com/highcharts.js" type="text/javascript"></script>
    <script src="https://code.highcharts.com/highcharts-more.js" type="text/javascript"></script>
    <script src="https://code.highcharts.com/modules/exporting.js" type="text/javascript"></script>
    <script src="https://code.highcharts.com/modules/export-data.js" type="text/javascript"></script>
    <script src="https://code.highcharts.com/modules/data.js" type="text/javascript"></script>
    <script src="https://code.highcharts.com/modules/accessibility.js" type="text/javascript"></script>
    <script src="https://code.highcharts.com/modules/no-data-to-display.js" type="text/javascript"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.18.1/moment.min.js" type="text/javascript"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment-timezone/0.5.13/moment-timezone-with-data-2012-2022.min.js" type="text/javascript"></script>
    <script src="/lib/js/dash/datatables.min.js" type="text/javascript"></script>

    <script src="/js/dash/general-script.min.js" type="text/javascript"></script>


    @await RenderSectionAsync("Scripts", required: false)
    <!-- Global site tag (gtag.js) - Google Analytics NIBSS Dashboard -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=G-SXR7X64YE3"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());

        gtag('config', 'G-SXR7X64YE3');
    </script>
</body>
</html>

